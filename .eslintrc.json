{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-trailing-spaces": "warn",
        "padded-blocks": ["warn", "never"],
        "no-multi-spaces": "warn"
    }
}
